#include <iostream>
using namespace std;

// Function to count trailing zeroes in n!
int countTrailingZeroes(int n) {
    int count = 0;
    while (n >= 5) {
        n /= 5;
        count += n;
    }
    return count;
}

// Function to find the smallest number whose factorial has at least n trailing zeroes
int findSmallestNumber(int n) {
    int low = 0, high = 5 * n, result = -1;
    while (low <= high) {
        int mid = low + (high - low) / 2;
        int trailingZeroes = countTrailingZeroes(mid);
        if (trailingZeroes >= n) {
            result = mid;
            high = mid - 1;
        } else {
            low = mid + 1;
        }
    }
    return result;
}

int main() {
    int n;
    cout << "Enter the number of trailing zeroes: ";
    cin >> n;
    int result = findSmallestNumber(n);
    if (result != -1) {
        cout << "The smallest number whose factorial has at least " << n << " trailing zeroes is: " << result << endl;
    } else {
        cout << "No such number exists." << endl;
    }
    return 0;
}

